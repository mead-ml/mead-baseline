{
    "task": "tagger",
    "batchsz": 20,
    "conll_output": "wnutresults.conll",
    "test_thresh": 10,
    "charsz": 16,
    "unif": 0.1,
    "preproc": {
	"mxlen": 60,
	"mxwlen": 40,
	"web-cleanup": false,
	"lower": true,
	"valsplit": 0.15
    },
    "backend": "tensorflow",
    "dataset": "wnut",
    "loader": {
	"reader_type": "default"
    },
    "model": {
	"model_type": "default",
	"cfiltsz": [
	    3
	],
	"hsz": 100,
	"wsz": 10,
	"dropout": 0.5,
	"rnntype": "blstm",
	"layers": 1,
        "crf_mask": true,
	"crf": 1
    },
    "word_embeddings": {"label": "glove-42B"},
    "train": {
	"epochs": 60,
	"optim": "sgd",
	"decay": 0,
	"eta": 0.01,
	"mom": 0.9,
	"patience": 20,
	"early_stopping_metric": "f1",
       "clip": 5.0,
        "span_type": "bio"
    }
}

