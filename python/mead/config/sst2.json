{
    "task": "classify",
    "batchsz": 50,
    "preproc": {
	"mxlen": 100,
	"rev": false,
	"clean": true
    },
    "backend": "tensorflow",
    "dataset": "SST2",
    "loader": {
	"reader_type": "default"
    },
    "unif": 0.25,
    "model": {
	"model_type": "default",
	"filtsz": [3,4,5],
	"cmotsz": 100,
	"dropout": 0.5,
	"finetune": true
    },
    "word_embeddings": {
	"label": "w2v-gn"
    },
    "train": {
	"epochs": 2,
	"optim": "adadelta",
	"eta": 1.0,
	"model_zip": true,
	"early_stopping_metric": "acc",
      "verbose": {"print": true, "file": "sst2-cm.csv"}
    }
}
