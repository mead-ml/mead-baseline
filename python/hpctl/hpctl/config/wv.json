{
    "task": "classify",
    "batchsz": 25,
    "preproc": {
        "mxlen": 100,
        "rev": false,
        "clean": true
    },
    "backend": "pytorch",
    "dataset": "SST2",
    "loader": {
        "reader_type": "default"
    },
    "unif": 0.25,
    "model": {
        "model_type": "default",
        "filtsz": [3, 4, 5],
        "cmotsz": 100,
        "dropout": 0.5,
        "finetune": true
    },
    "word_embeddings": {
        "label": {
            "type": "grid",
            "values": ["glove-6B-300", "glove-6B-100"]
        }
    },
    "train": {
        "epochs": 2,
        "optim": "adadelta",
        "eta": 1.0,
        "model_base": "./models/sst2",
        "early_stopping_metric": "acc",
        "verbose": true
    }
}
