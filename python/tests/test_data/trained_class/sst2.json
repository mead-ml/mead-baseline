{
    "batchsz": 50,
    "preproc": {
        "mxlen": 100,
        "rev": false,
        "clean": true
    },
    "backend": "tensorflow",
    "dataset": "SST2",
    "loader": {
        "eval": true,
        "reader_type": "default"
    },
    "unif": 0.25,
    "model": {
        "model_type": "nbow",
        "hsz": 2,
        "dropout": 0.5,
        "finetune": true
    },
    "word_embeddings": {
        "dsz": 3
    },
    "train": {
        "epochs": 2,
        "optim": "sgd",
        "eta": 0.1,
        "model_base": "./models/sst2",
        "ema_decay": 0.99,
        "early_stopping_metric": "acc"
    }
}
