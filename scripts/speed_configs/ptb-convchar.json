{
    "task": "lm",
    "batchsz": 20,
    "unif": 0.05,
    "nbptt": 35,
    "charsz": 16,
    "preproc": {
        "mxwlen": 40,
        "lower": true
    },
    "backend": "tensorflow",
    "dataset": "ptb",
    "loader": {
        "reader_type": "default"
    },
    "model": {
        "model_type": "convchar",
        "cfiltsz": [1,2,3,4,5,6,7],
        "nfeat_factor": 50,
        "hsz": 650,
        "layers": 2,
        "gating": "highway",
        "num_gates": 2
    },
    "word_embeddings": {
        "dsz": 0
    },
    "train": {
        "epochs": 1,
        "decay_rate": 1.6,
        "patience": 40000,
        "optim": "sgd",
        "start_decay_epoch": 10,
        "decay_type": "zaremba",
        "eta": 1.0,
        "mom": 0.0,
        "do_early_stopping": true,
        "clip": 5.0
    }
}
